# Ash initialization.

test -z "$EDITOR" && {	# Don't repeat in subshells.

umask 022

# Favourite editor and pager, search path for binaries, etc.
export EDITOR=vi
export PAGER=more
export PATH=$HOME/bin:/usr/local/bin:/bin:/usr/bin

} # End of no-repeat.

